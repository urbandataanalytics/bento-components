{
  "name": "@uda/bento-components",
  "description": "uDA component library",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "react",
    "react-component",
    "ui",
    "library",
    "component"
  ],
  "author": "Urban Data Analytics",
  "homepage": "https://github.com/urbandataanalytics/bento-components",
  "bugs": {
    "url": "https://github.com/urbandataanalytics/bento-components/issues"
  },
  "private": false,
  "scripts": {
    "storybook": "yarn build:icons && start-storybook -p 9009 -s public",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build",
    "build": "yarn clean && yarn build:icons && yarn build:lib",
    "build:lib": "yarn copy:lib && cross-env NODE_ENV=\"production\" babel --out-dir lib --ignore **/*.stories.js,**/*.test.js,**/serviceWorker.js,**/setupTests.js src",
    "build:icons": "babel-node scripts/svgIcon.js",
    "build:storybook": "yarn build:icons && build-storybook -s public",
    "build:local": "yarn build:icons && yarn build:lib && yalc publish --push",
    "copy:lib": "copyfiles -u 1 \"./src/icons/**\" --exclude \"./src/icons/svg/**\" lib && yarn copy:ts",
    "copy:ts": "copyfiles -u 1 \"./src/**/*.d.ts\" lib",
    "deploy:storybook": "cross-env BABEL_TARGET=\"storybook\" storybook-to-ghpages",
    "test": "jest",
    "test:ci": "yarn build:icons && jest --ci --coverage",
    "test:updateSnapshot": "yarn test --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix src/**/*.js",
    "lint:css": "stylelint \"./src/components/**/*.js\"",
    "prettier:fix": "prettier src/components/**/*.js --write",
    "clean": "rimraf lib",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{spec,d}.{ts,tsx}\""
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md",
    ".babelrc",
    ".eslintrc",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/urbandataanalytics/bento-components"
  },
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "hello@ghbot.com",
    "commitMessage": "Deploy Storybook to GitHub Pages [skip ci]"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn prettier:fix"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@adeira/babel-preset-adeira": "^0.3.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@storybook/storybook-deployer": "^2.8.6",
    "@styled-system/prop-types": "5.1.2",
    "@styled-system/props": "5.1.4",
    "@styled-system/theme-get": "5.1.2",
    "@svgr/cli": "^5.4.0",
    "@svgr/core": "^2.0.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^25.2.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-es2015": "^6.24.1",
    "camelcase": "^6.0.0",
    "capitalize": "^2.0.3",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "dtslint": "^2.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "jest": "25.5.4",
    "jest-enzyme": "^7.1.2",
    "jest-styled-components": "^6.3.4",
    "jsdom": "^16.2.2",
    "mkdirp": "^1.0.4",
    "nyc": "^15.1.0",
    "playroom": "^0.18.1",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^4.3.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^24.1.0",
    "@ag-grid-community/core": "^24.1.0",
    "@ag-grid-community/react": "^24.1.1",
    "embla-carousel": "^4.0.6",
    "rc-slider": "^9.3.1",
    "react-cool-onclickoutside": "1.5.5"
  }
}
